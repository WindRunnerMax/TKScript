{
  "name": "force-copy",
  "version": "1.0.0",
  "author": "Czy",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development rspack build --watch",
    "build": "rm -rf build && rspack build",
    "dev:rollup": "cross-env NODE_ENV=development rollup -wc",
    "dev:gecko": "cross-env NODE_ENV=development PLATFORM=gecko rspack build --watch",
    "build:gecko": "rm -rf build && cross-env PLATFORM=gecko rspack build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/WindrunnerMax/TKScript"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.50.0",
    "copy": "workspace: *",
    "copy-currency": "workspace: *",
    "laser-utils": "0.0.2-alpha.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-html": "^1.0.2",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rspack/cli": "^0.2.5",
    "@rspack/plugin-html": "^0.2.5",
    "@types/chrome": "^0.0.241",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "less": "^3.0.0",
    "less-loader": "6.0.0",
    "sass": "^1.52.3",
    "sass-loader": "^13.3.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss,less}": [
      "stylelint --fix"
    ]
  }
}
